{{#each this}}
<h2 class="card-title">{{name}}</h2>
<div class="card">
  <div class="card-body">
    <p class="card-text"><b>Capital:</b> {{capital}}</p>
    <p class="card-text"><b>Population:</b> {{population}}</p>
    <ul class="languages-list"><b>Languages:</b>
      {{#each languages}}
      <li class="languages-item">{{this.name}}</li>
      {{/each}}
    </ul>
  </div>
  <div class="card-img">
    <img src="{{flag}}" alt="{{name}}" width="300">
  </div>

</div>
{{/each}}